@import "./history.wxss";
.list{padding:0;}
.title{font-size: 36rpx;}
.bdy{display: block;}
.bdy>image{width: 100%;}
.bdy>view{padding-left: 0;padding:20rpx}
.txt_box{padding:15rpx 0 0 ;}
.address{padding:0 20rpx 20rpx;}
.txt{padding:15rpx 0 0;}
.bottom>view.b_y{background: #f2ece5;}
.bottom>view.b_h{background: #eaeaea;}
.b_pj{background: rgba(0,0,0,0.8);position: fixed;left: 0;top: 0;width: 100%;height: 100%;}
.pj_bdy{width: 83vw;background: #fff;border-radius: 12rpx;margin:20vw 5vw 5vw;padding:2vw;}
.pj_bdy .title{font-size: 30rpx;border-bottom: 1rpx solid #f9f9f9;padding:20rpx;text-align: center;margin:0 20rpx}
.pj_btn{padding:40rpx 0;}
.pj_btn>text{display: inline-block;padding:10rpx 20rpx;background: #f9f9f9;color: #999;}
.pj_btn>text:nth-child(2){float: right;}
.jdt{display: flex;margin: 0 40rpx 80rpx;position: relative;}
.jdt>text{flex: 1;background: #eee;height: 10rpx;}
.jdt>text:first-child{border-radius: 5rpx 0 0 5rpx;}
.jdt>text:nth-child(10){border-radius: 0 5rpx  5rpx 0;}
.jdt>.jdt_btn{position: absolute;top: -11rpx;z-index: 9;}
.jdt>.jdt_btn>text{display: inline-block;width: 20rpx;height: 20rpx;border-radius: 50%;background: #fff;border: 1rpx solid #eee;}
.jdt>.jdt_btn>view{width: 100rpx;margin-left: -40rpx;text-align: center;color: #999;}
#pj_cent{border-radius: 10rpx;border:1rpx solid #eee;padding:20rpx;height: 160rpx;width:auto;}
.pj_bdy>.btn{margin:40rpx 0 20rpx;background: #000;color: #ffc310;text-align: center;padding:30rpx;}
.b_pj>.gb{width: 40rpx;height: 40rpx;line-height: 40rpx;text-align: center;font-size: 36rpx;padding:10rpx;border-radius: 50%;border:2rpx solid #fff;color: #fff;margin:0 auto;}
.pj_history{padding:20rpx 10rpx;}
.pj_history>.history_title{font-weight: bold;}
.pj_history>.lists{padding:20rpx 0;color: #999;}
.pj_history>.lists>text{float: right;}
.pj_history>.bq_list{padding:20rpx 0;}
.pj_history>.bq_list>view{display: inline-block;margin-right: 20rpx;border:1rpx solid #eee;padding:10rpx 20rpx;border-radius: 40rpx;position: relative;color: #999;}
.pj_history>.bq_list>view>text{display: inline-block;width: 26rpx;height: 26rpx;font-size: 16rpx;text-align: center;line-height: 26rpx;color: #fff;background: #999;right: -5rpx;top: -5rpx;position: absolute;border-radius: 50%;}